<Window x:Class="Prometheus.Proletarians.WpfCore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prometheus.Proletarians.WpfCore"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu>
            <MenuItem Header="Обзвон" >
                <MenuItem Header="Спискок обзвона" Click="OnInvationPrifiles"></MenuItem>
            </MenuItem>
        </Menu>
        <DockPanel Grid.Column="1" Grid.Row="1">
            <Frame x:Name="_mainFrame" NavigationUIVisibility="Visible" Source=""/>
        </DockPanel>
    </Grid>
</Window>
